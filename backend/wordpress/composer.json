{
	"name": "itsamoreh/nextjs-headless-wordpress",
	"description": "Project template for headless WordPress / Next.js monorepo.",
	"minimum-stability": "dev",
	"prefer-stable": true,
	"repositories": [
		{
			"type": "composer",
			"url": "https://wpackagist.org"
		},
		{
			"type": "vcs",
			"url": "git@github.com:itsamoreh/bring-your-own-blocks"
		},
		{
			"type": "git",
			"url": "https://github.com/findcanary/wp-graphql-the-seo-framework.git"
		},
		{
			"type": "git",
			"url": "https://github.com/sybrew/the-seo-framework.git"
		}
	],
	"require": {
		"findcanary/wp-graphql-the-seo-framework": "master",
		"itsamoreh/bring-your-own-blocks": "^0.1.0",
		"pristas-peter/wp-graphql-gutenberg": "^0.3.11",
		"sybrew/the-seo-framework": "^4.2.0",
		"wpackagist-plugin/advanced-custom-fields": "^5.11",
		"wpackagist-plugin/wp-graphql": "^1.6.7",
		"wp-graphql/wp-graphql-acf": "^0.5.3"
	}
}
